 @model Clinic_web_app.Models.AdminOrderDetail

<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1 class="m-0">Rent Equipment Detail</h1>
                </div><!-- /.col -->
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="#">Home</a></li>
                        <li class="breadcrumb-item active">Staff Account</li>
                    </ol>
                </div><!-- /.col -->
            </div><!-- /.row -->
        </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->
    <!-- Main content -->
    <section class="content">
        <div class="container-fluid">
            <dl class="row">
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Quantity)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Quantity)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Purpose)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Purpose)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Equipment)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Equipment.EquipmentName)
                </dd>
                <dt class="col-sm-2">
                    Staff who rents
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.OrderDetail.Account.Username);
                </dd>
            </dl>
          
            <div>
                <a asp-action="Edit" asp-route-id="@Model.OrderId">Edit</a> |
                <a asp-action="Index">Back to List</a>
            </div>
        </div>
    </section>
    <!-- /.content -->
</div>


